{
  "name": "lexiko",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development DEBUG=APP_NAME:* nodemon ./bin/www",
    "start:dev:win": "SET \"NODE_ENV=development\" SET DEBUG=myapp:* && nodemon ./bin/www",
    "db:create:migration": "npx sequelize migration:generate --name ",
    "db:migrate": "npx sequelize db:migrate",
    "db:rollback": "npx sequelize db:migrate:undo",
    "postinstall": "npx sequelize db:migrate"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "connect-flash": "^0.1.1",
    "connect-pg-simple": "^7.0.0",
    "connect-sqlite3": "^0.9.13",
    "cookie-parser": "~1.4.4",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.3",
    "flash": "^1.1.0",
    "glob-stream": "^7.0.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.15.2",
    "popups": "^1.1.3",
    "pug": "^3.0.2",
    "request": "^2.88.2",
    "sequelize": "^6.25.1",
    "sequelize-cli": "^6.5.1",
    "socket.io": "^4.5.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
