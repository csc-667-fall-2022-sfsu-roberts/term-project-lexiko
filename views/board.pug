mixin board(gameBoardTiles, handTiles)
    - var s = 0
    - var t = 0
    - var stuff = [["A", 10],["E", 5],["C", 7],["S", 3],["P", 4],["L", 8],["A", 10]]

    include tile
    
    block content
        .wrapper.board-wrapper
            .board
                while s < 225
                    .board-space(id=s++)
                        .board-tile
        .wrapper.hand-wrapper
            .hand
                each tileVal in handTiles
                    .hand-space(id = t++)
                        .hand-tile
                            +tile(tileVal[0], tileVal[1])